{
  "name": "Smichal Reader",
  "description": "Smichal's epub book reader",
  "version": "1.00",
  "manifest_version": 2,
  "app": {
    "background": {
      "scripts": ["js/background.js"]
    }
  },
  "icons": {"16": "assets/ico2.png", "128": "assets/ico2.png"},
  "permissions": [
    "fullscreen",
    {"fileSystem": ["retainEntries"]},
    "storage"
  ]
}